{
    "name": "jest-test",
    "version": "1.0.0",
    "description": "jest-test",
    "main": "index.js",
    "scripts": {
        "test": "jest"
    },
    "author": "Heat Lai",
    "license": "ISC",
    "dependencies": {
        "dotenv": "^8.2.0",
        "jest": "^25.1.0",
        "jest-jenkins-reporter": "^1.0.2",
        "puppeteer": "^2.1.1"
    },
    "jest": {
        "testMatch": [
            "**/*.spec.js"
        ],
        "testResultsProcessor": "jest-jenkins-reporter",
        "moduleNameMapper": {
            "mylib/(.*)": "<rootDir>/lib/$1"
        }
    },
    "_moduleAliases": {
        "mylib": "lib"
    }
}
